em ,i{
	font-style: normal !important;
}
ul{
	list-style: none;
}

.law__box{

	
	a{
		color:#333;
		&:hover{
			text-decoration: none;   
		}
	}
	
	.box__ul{
		font-size:16px;
		li{
			margin:20px 0px;
			border:1px solid #ccc;
			line-height:30px;
			&:hover{
				background:#eee;
			}
		}
		span{
			width:20px;
			height:25px;
			
			display: inline-block;
			background:url(../../images/account-images.png) -170px -104px no-repeat ;
			
		}
		a{
			
			padding-left:15px;
			
		}
		.ul__i{
			float:right;
			font-size:20px;
			line-height:40px;
			margin-right:20px;
		}
	}
	.text{
		text-align: right;
		margin-right:70px;
	}
}