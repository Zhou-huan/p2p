// 公共样式
.wlty{
    width:80px;
    height:20px;
    background:#a8d1ff;
    text-align:center;
    line-height:20px;
    position: absolute;
    top:0;
    left:0px;
    color:#fff;  
    z-index:1;
}
.progress{
    position: relative;
}

.center{
    text-align:center;
    position: absolute;
    left:35%;
    top:8%;
    z-index: 1;
}

.starbox{
    width: 120px;
    height:50px;
    margin:0 auto;
    background: url(../../images/xx2.png) no-repeat 0 0;
    position: relative;
    transform: scale(0.75);
    margin-top:35px;
    .star{
        width: 100%;
        height: 50px;
        background: url(../../images/xx1.png) no-repeat 0 0px;
        position: absolute;
        top:0;
        left:0;
    }
}
.mrl{
    margin:0 auto;
    margin-top:10px;
    height:25px;
    background-color: #ccc;
    a{
        color:#FFF;
        line-height:25px;
        &:hover{
            text-decoration: none;
        }
    }
}




.list__main{
    .main__head{
        padding:30px 0;
        line-height:1px;
    }
    .main__content{
        margin:0 auto;
        text-align:center;
        font-size:18px;  
        position: relative; 
        .content__bar{
            p{
                line-height:0;
                height:0px;
                margin:0;
                padding:0;
                font-size:14px;
                margin-top:28px;
              
            }    
        }
        .right__txt{
            position: absolute;
            top:0;
            left:90px;
        }
       
        .right__txt{  
            &:hover{color:#F00 !important;}  
        }
        .left__txt{  
            &:hover{color:#F00 !important;}  
        }
         
    }
    
}
// 分页
.paging{
   display:flex;
   justify-content:center;
}